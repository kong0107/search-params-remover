{
    "manifest_version": 3,
    "name": "Search Params Remover",
    "version": "0.2.0",
    "description": "a browser extension helps you surf without utm_source and fbclid",
    "author": "kong0107",
    "icons" : {
        "128": "icon.png"
    },
    "permissions": [
        "storage",
        "declarativeNetRequest"
    ],
    "host_permissions": [
        "*://*/*"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "options_page": "options.html"
}
